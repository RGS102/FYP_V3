<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rossg_000.fyp_v3">

    <!--
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    -->


    <!-- android:theme="@style/AppTheme" -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/trident3"
        android:label="TRIDENT"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tasks"
            android:label="Tasks"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Journal"
            android:label="Journal"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Stretches"
            android:label="Stretches"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Exercise"
            android:label="Exercises"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Meditation"
            android:label="Meditation"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".PopUpInfo"
            android:label="Activity Information"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".CompleteOrFail"
            android:label="Progress Report"
            android:theme="@style/Theme.AppCompat.Dialog.Alert"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Rewards"
            android:label="Rewards"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".HowTo"
            android:label="FAQ"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>