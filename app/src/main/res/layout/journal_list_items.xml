<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp">


    <TextView
        android:id="@+id/JournalTaskName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalLevelString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:text="Level"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="25dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalLevelString"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalAttemptString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="150dp"
        android:layout_marginTop="25dp"
        android:text="Attempt:"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalAttempt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="25dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalAttemptString"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalTaskAction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="45dp"
        android:text="Ran"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalTaskProgress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="20"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalTaskAction"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalTaskMeasurement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="miles"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalTaskProgress"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalForString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="for"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalTaskMeasurement"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalDuration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="20"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalForString"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalTimeMeasurement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="minutes"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalDuration"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalLevelUpOrDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="65dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalTimeString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="85dp"
        android:text="Time:"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalCurrentTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="85dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalTimeString"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalDateString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="150dp"
        android:layout_marginTop="85dp"
        android:text="Date:"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/JournalCurrentDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="85dp"
        android:text="TextView"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/JournalDateString"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>